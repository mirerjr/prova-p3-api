{
    "name": "prova-p3-api",
    "version": "1.0.0",
    "public": true,
    "build": {
        "src": "api/index.js",
        "use": "@vercel/node"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ],
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/" 
        }
    ] 
}