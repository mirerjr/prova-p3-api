{
    "name": "prova-p3-api",
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ],
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/" 
        }
    ] 
}